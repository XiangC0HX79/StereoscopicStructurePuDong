<?xml version="1.0" encoding="utf-8"?>
<s:VGroup xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx">
	
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[		
			public static const DESCRIPTION:String = "单位基本情况";
			public static const FUNCDIVISION:String = "功能分区";
			public static const SECURITYORG:String = "安保组织结构";
			public static const TACTICS:String = "战术要点";
		]]>
	</fx:Script>
	<s:Button label="{DESCRIPTION}" skinClass="app.skin.skinSubMenuButton" buttonMode="true" width="175" height="36" buttonDown="dispatchEvent(new Event(DESCRIPTION))"/>
	<s:Button label="{FUNCDIVISION}" skinClass="app.skin.skinSubMenuButton" buttonMode="true" width="175" height="36" buttonDown="dispatchEvent(new Event(FUNCDIVISION))"/>
	<s:Button label="{SECURITYORG}" skinClass="app.skin.skinSubMenuButton" buttonMode="true" width="175" height="36" buttonDown="dispatchEvent(new Event(SECURITYORG))"/>
	<s:Button label="{TACTICS}" skinClass="app.skin.skinSubMenuButton" buttonMode="true" width="175" height="36" buttonDown="dispatchEvent(new Event(TACTICS))"/>
</s:VGroup>
